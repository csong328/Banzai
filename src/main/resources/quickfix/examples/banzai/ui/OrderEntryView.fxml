<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane hgap="5" layoutY="5.0" prefHeight="68.0" prefWidth="600.0" vgap="5"
          stylesheets="/quickfix/examples/banzai/ui/banzi.css"
          xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="quickfix.examples.banzai.ui.OrderEntryController">
    <Label text="Symbol" GridPane.columnIndex="0"/>
    <Label text="Quantity" GridPane.columnIndex="1"/>
    <Label text="Side" GridPane.columnIndex="2"/>
    <Label text="Type" GridPane.columnIndex="3"/>
    <Label text="Limit" GridPane.columnIndex="4"/>
    <Label text="Stop" GridPane.columnIndex="5"/>
    <Label text="TIF" GridPane.columnIndex="6"/>
    <TextField fx:id="symbolTextField" prefHeight="25.0" prefWidth="88.0"
               GridPane.columnIndex="0" GridPane.rowIndex="2"/>
    <TextField fx:id="quantityTextField" prefHeight="25.0" prefWidth="75.0"
               GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <ComboBox fx:id="sideComboBox" editable="false" prefHeight="25.0" prefWidth="69.0"
              GridPane.columnIndex="2" GridPane.rowIndex="2"/>
    <ComboBox fx:id="typeComboBox" editable="false" prefHeight="25.0" prefWidth="87.0"
              GridPane.columnIndex="3" GridPane.rowIndex="2"/>
    <TextField fx:id="limitPriceTextField" prefHeight="25.0" prefWidth="59.0"
               GridPane.columnIndex="4" GridPane.rowIndex="2"/>
    <TextField fx:id="stopPriceTextField" prefHeight="25.0" prefWidth="80.0"
               GridPane.columnIndex="5" GridPane.rowIndex="2"/>
    <ComboBox fx:id="tifComboBox" editable="false" prefHeight="25.0" prefWidth="78.0"
              promptText="Day" GridPane.columnIndex="6" GridPane.rowIndex="2"/>
    <ComboBox fx:id="sessionComboBox" editable="false" prefHeight="25.0" prefWidth="550.0"
              GridPane.columnIndex="0" GridPane.columnSpan="4" GridPane.rowIndex="3"/>
    <Button fx:id="newButton" mnemonicParsing="false" onAction="#onNewOrder"
            prefHeight="25.0" prefWidth="80.0" text="New" GridPane.columnIndex="4"
            GridPane.rowIndex="3"/>
    <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelOrder"
            text="Cancel" GridPane.columnIndex="5" GridPane.rowIndex="3"/>
    <Button fx:id="replaceButton" mnemonicParsing="false" onAction="#onReplaceOrder"
            text="Replace" GridPane.columnIndex="6" GridPane.rowIndex="3"/>
</GridPane>