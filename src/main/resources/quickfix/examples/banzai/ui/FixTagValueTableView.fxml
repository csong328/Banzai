<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<TableView fx:id="tagValueTable" stylesheets="/quickfix/examples/banzai/ui/banzi.css"
           xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="quickfix.examples.banzai.ui.TagValueTableController">
    <columns>
        <TableColumn prefWidth="40" text="Tag">
            <cellValueFactory>
                <PropertyValueFactory property="tag"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="125" text="Value">
            <cellValueFactory>
                <PropertyValueFactory property="value"/>
            </cellValueFactory>
        </TableColumn>
    </columns>
</TableView>